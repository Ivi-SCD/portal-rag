{
  "name": "canais-chatbot-rag-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": ".",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:non-prd": "tsc && vite build --mode staging",
    "lint:js": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:css": "stylelint --fix ./src/**/*.{css,scss}",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,scss,md,json}\" ",
    "lint": "npm run lint:format && npm run lint:js && npm run lint:css",
    "preview": "npm run lint && npm run test && npm run build",
    "open:preview": "vite preview",
    "prepare": "husky",
    "test": "vitest --coverage",
    "test:ui": "vitest --ui --coverage"
  },
  "dependencies": {
    "@axa-fr/react-oidc": "^7.22.4",
    "@diana-kit/react": "^0.8.39",
    "@diana-kit/web": "^0.8.39",
    "@sicredi/authentication": "^4.30.4",
    "@sicredi/header-navigation-portal": "^1.5.4",
    "axios": "^1.7.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.22.3",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@originjs/vite-plugin-federation": "^1.3.5",
    "@sicredi/dev-tools": "^4.30.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^15.0.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.5.0",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "stylelint": "^16.4.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "typescript": "^5.2.2",
    "validate-branch-name": "^1.3.0",
    "vite": "^5.2.0",
    "vitest": "^1.5.0"
  }
}
